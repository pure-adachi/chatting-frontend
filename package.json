{
  "name": "chatting-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material/react-button": "^0.4.2",
    "@material/react-text-field": "^0.5.1",
    "@material/react-top-app-bar": "^0.5.0",
    "@material/theme": "^0.40.1",
    "apollo-boost": "^0.1.15",
    "flat": "^4.1.0",
    "graphql": "^14.0.0",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "react": "^16.4.2",
    "react-apollo": "^2.1.11",
    "react-dom": "^16.4.2",
    "react-intl": "^2.7.0",
    "react-loading-overlay": "^0.3.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.5",
    "react-toastify": "^4.4.0",
    "url-polyfill": "^1.0.14"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src/styles --include-path ./node_modules ./src/styles/App.scss -o ./src/styles",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src/styles --include-path ./node_modules --watch ./src/styles/App.scss ./src/styles/App.css",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
